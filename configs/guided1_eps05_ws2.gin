# Train configuration
train.seed=10
train.batch_size=8
train.lr=0.0001
train.wd=0.01
train.n_epochs=15
train.nummodalities=2
train.other_callbacks=['BiasMitigationStrong']

ModelCheckpoint.monitor='val_f1'

BiasMitigationStrong.epsilon=0.5
BiasMitigationStrong.curation_windowsize=2
BiasMitigationStrong.starting_epoch=1
BiasMitigationStrong.branchnames=['sar', 'opt']

ProgressionCallback.other_metrics=['d_speed', 'cls_sar', 'cls_opt', 'curation_mode', 'caring_modality']

WBLoggingCallback.on=True
WBLoggingCallback.log_frequency=100
WBLoggingCallback.project='balanced_learning_v2'
WBLoggingCallback.other_metrics=['d_speed', 'cls_sar', 'cls_opt', 'curation_mode', 'caring_modality']


